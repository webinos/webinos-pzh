{ "authentication" :
    { "google" :
      { "enabled"        : true
      , "callbackURL"    : ""
      , "apiKey"         : null
      }
    , "yahoo" :
      { "enabled"        : true
      , "callbackURL"    : ""
      }
    , "openid" : 
      { "enabled"        : false
      , "callbackURL"    : "" 
      }
    , "facebook" : 
      { "enabled"        : false
      , "clientID"       : ""
      , "clientSecret"   : ""
      , "callbackURL"    : ""
      }
    , "twitter" :
      { "enabled"        : false
      , "consumerKey"    : ""
      , "consumerSecret" : ""
      , "callbackURL"    : ""
      , "oauth"       : 
        { "accessToken"       : null
        , "accessTokenSecret" : null
        }
      }
    }
, "csp" :
    { "defaultPolicy": 
      { "default-src" : ["'self'"]
      , "connect-src" : "*"
      , "script-src"  : ["'self'", "https://ajax.googleapis.com", "'unsafe-inline'"]
      , "style-src"   : ["'self'", "'unsafe-inline'"]
      , "img-src"   : "*"
      }
    }
}
